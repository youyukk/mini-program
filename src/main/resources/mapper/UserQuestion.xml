<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE mapper PUBLIC
        "-//mybatis.org//DTD com.example.Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="UserQuestionMapper">

	<insert id="addUserQuestion" parameterType="java.util.HashMap">
		insert into user_answer(openid,question_id,user_answer,create_time)
		values(#{openId},#{questionId},#{answer},#{createTime})
	</insert>

</mapper>